---
import albums from '../data/albums.json'

const artists = albums.albums.map((album) => (
                    album.artist));



const item = artists.map(
    (artist)  => (
        {"@type": "Thing", "name": artist}
      )
    )

---
<script type="application/ld+json" set:html={JSON.stringify({
  "@context": "https://schema.org",
  "@type": "WebPage",
  "about": item
  })}></script>