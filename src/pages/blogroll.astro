---
// COMPONENTS

import Layout from '../layouts/Layout.astro';
import {Content as Blogroll} from '../data/blogroll.mdx';

// METADATA VARIABLES

let title = 'Blogroll';
let description = 'This is my blogroll showcasing some of my favourite blogs on the internet.';

---
<Layout {title} {description}>
          <h1>{title}</h1>
          <p>{description}</p>
          <Blogroll />
      </Layout>